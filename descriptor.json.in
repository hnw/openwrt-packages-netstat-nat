{
  "package": {
    "subject": "${BINTRAY_USER}",
    "repo": "${BINTRAY_REPO}",
    "name": "${PKG_NAME}",
    "desc": "A netstat variant for NAT connections",
    "website_url": "http://tweegy.nl/projects/netstat-nat/",
    "issue_tracker_url": "https://github.com/hnw/openwrt-packages-${PKG_NAME}/issues",
    "vcs_url": "https://github.com/hnw/openwrt-packages-${PKG_NAME}.git",
    "licenses": [
      "GPL-2.0"
    ],
    "public_download_numbers": false,
    "public_stats": false
  },
  "version": {
    "name": "${VERSION}",
    "desc": "${DESC}",
    "vcs_tag": "${VCS_TAG}",
    "released": "${RELEASED}",
    "github_use_tag_release_notes": false,
    "gpgSign": false
  },
  "files": [
    {
      "includePattern": "${TRAVIS_BUILD_DIR}/pkgs-for-bintray/(.*\\.${UPLOAD_EXT})",
      "uploadPattern": "${DISTRO}/${ARCH}${UPLOAD_DIR_SUFFIX}/$1",
      "matrixParams": {
        "override": 1
      }
    }
  ],
  "publish": true
}
